{
  "name": "records",
  "version": "0.2.0",
  "description": "Record collection web app and back end server monorepo",
  "scripts": {
    "back:dev": "npm start --prefix back -- $npm_package_config_ports_back",
    "front:dev": "npm run dev --prefix front -- --port $npm_package_config_ports_front",
    "start": "npx concurrently \"npm:back:dev\" \"npm:front:dev\""
  },
  "config": {
    "ports": {
      "back": 2600,
      "front": 2601
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-brianzelip:brianzelip/records.git"
  },
  "keywords": [
    "vinyl records",
    "monorepo"
  ],
  "author": {
    "name": "Brian Zelip",
    "email": "bzelip@protonmail.com",
    "url": "https://zelip.me/"
  },
  "license": "MIT"
}
